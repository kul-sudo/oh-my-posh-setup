{
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#444444",
          "foreground": "#e5e5e5",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0077c2",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#40c4ff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " $ ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
